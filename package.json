{
    "name": "otc-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint:fix": "npm run format && eslint \"{src,apps,libs}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs -d src/modules/database/configs/typeorm.config.ts",
        "typeorm:entity:create": "typeorm-ts-node-commonjs entity:create src/modules/database/entities/$npm_config_name",
        "migration:create": "typeorm migration:create src/modules/database/migrations/$npm_config_name",
        "migration:generate": "npm run typeorm migration:generate src/modules/database/migrations/$npm_config_name",
        "migration:show": "npm run typeorm migration:show",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.645.0",
        "@aws-sdk/s3-request-presigner": "^3.645.0",
        "@coral-xyz/anchor": "^0.29.0",
        "@metamask/eth-sig-util": "^7.0.1",
        "@nestjs/bull": "^10.1.1",
        "@nestjs/common": "^10.4.1",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^10.3.10",
        "@nestjs/schedule": "^4.0.1",
        "@nestjs/swagger": "^7.4.0",
        "@nestjs/throttler": "^4.2.1",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.3.10",
        "@project-serum/anchor": "^0.26.0",
        "@solana/spl-token": "^0.4.8",
        "@solana/web3.js": "^1.91.4",
        "bull": "^4.12.2",
        "cache-manager": "^5.2.3",
        "cache-manager-redis-store": "^3.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "decimal.js": "^10.4.3",
        "ethers": "5.7.1",
        "ioredis": "^5.3.2",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "pg": "^8.11.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/lodash": "^4.17.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.3.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
